{
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:flowtype/recommended",
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true,
    "es6": true,
  },
  "plugins": [
    "flowtype",
  ],
  "rules": {
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-caller": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-control-regex": 0,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-multi-spaces": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-wrappers": 2,
    "no-octal-escape": 2,
    "no-process-exit": 0,
    "no-proto": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": [0, {builtinGlobals: false}],
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-undef-init": 2,
    "no-underscore-dangle": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": [2, {args: "none"}],
    "no-with": 2,
    "camelcase": [2, {properties: "never"}],
    "comma-dangle": [1, "always-multiline"],
    "comma-spacing": 2,
    "consistent-return": 0,
    "curly": [2, "multi-line"],
    "dot-notation": [2, {allowKeywords: true}],
    "eol-last": 2,
    "no-extra-parens": [2, "functions"],
    "eqeqeq": [2, "always", {null: "ignore"}],
    "key-spacing": [2, {mode: "minimum", beforeColon: false, afterColon: true}],
    "keyword-spacing": 2,
    "max-len": [2, 120, 4, {ignoreUrls: true}],
    "new-cap": 0,
    "new-parens": 0,
    "quotes": [2, "double", "avoid-escape"],
    "quote-props": [2, "consistent-as-needed"],
    "semi": [2, "never"],
    "space-infix-ops": 2,
    "space-unary-ops": [2, {words: true, nonwords: false}],
    "yoda": [2, "never"],
  },
}
